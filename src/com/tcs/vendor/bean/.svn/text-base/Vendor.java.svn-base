package com.tcs.vendor.bean;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class Vendor {
	
	private String v_id;
	private String v_name;
	private String v_pwd;
	private String v_email;
	private long v_phone;
	private int v_active;
	private String v_type;
	
	public Vendor()
	{
		
	}
	
	/* 			For Creating new Vendor				*/
	
	public Vendor(String v_name, String v_email,long v_phone,String v_type) {
		super();
		
		this.v_name = v_name;
		this.v_email = v_email;
		this.v_phone = v_phone;
		this.v_type = v_type;
	}
	
	/*					For displaying all vendors	*/
	public Vendor(String v_id,String v_name, String v_email,long v_phone,String v_type) {
		super();
		
		this.v_id=v_id;
		this.v_name = v_name;
		this.v_email = v_email;
		this.v_phone = v_phone;
		this.v_type = v_type;
	}


	public Vendor(String v_id, String v_name, String v_pwd, String v_email,
			long v_phone, int v_active, String v_type) {
		super();
		this.v_id = v_id;
		this.v_name = v_name;
		this.v_pwd = v_pwd;
		this.v_email = v_email;
		this.v_phone = v_phone;
		this.v_active = v_active;
		this.v_type = v_type;
	}

	public String getV_id() {
		return v_id;
	}

	public void setV_id(String v_id) {
		this.v_id = v_id;
	}

	public String getV_name() {
		return v_name;
	}

	public void setV_name(String v_name) {
		this.v_name = v_name;
	}

	public String getV_pwd() {
		return v_pwd;
	}

	public void setV_pwd(String v_pwd) {
		this.v_pwd = v_pwd;
	}

	public String getV_email() {
		return v_email;
	}

	public void setV_email(String v_email) {
		this.v_email = v_email;
	}

	public long getV_phone() {
		return v_phone;
	}

	public void setV_phone(long v_phone) {
		this.v_phone = v_phone;
	}

	public int getV_active() {
		return v_active;
	}

	public void setV_active(int v_active) {
		this.v_active = v_active;
	}

	public String getV_type() {
		return v_type;
	}

	public void setV_type(String v_type) {
		this.v_type = v_type;
	}

	public String toString()
	{
		return v_id+" , "+v_name+" , "+v_email+" , "+v_phone+" , "+v_type;
	}
}

